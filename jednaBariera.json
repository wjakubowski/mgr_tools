{
	"symulation":{
		"doSymulation":true,
		"oommfPath":"/net/archive/groups/plggspinsym/WJ/OOMMF-SWAG/oommf.tcl",
		"grant":"grant009",
		"partition":"plgrid",
		"maxSimulationTime":"4:00:00",
		"mifParameters":{
			"mxfree":						{"value":"[-math.sqrt(3)/2]", "precision": 4, "type": "script", "verbous": true},

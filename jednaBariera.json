			"VProfileType":					{"value":0, "precision": 0, "type": "value", "verbous": true},
			"Voltage":						{"value":"[0.16]", "precision": 4, "type": "script", "verbous": true},
						
			"stoppingTime":					{"value":1e-8, "type": "value", "verbous": false},
			"timeStep":						{"value":1e-13, "type": "value", "verbous": false},
			"scheduleStep":					{"value":10, "type": "value", "verbous": false},
			"vecFieldScheduleStep":			{"value":1000, "type": "value", "verbous": false}
		}
	},
	"plot":{
		"doPloting":true,
		"zSlice":1,
		"framesPerSecond":5,
		"drawSingleFrames":false,
		"odtPlots":{
				"1":{"variableNameX":"Simulation time", "variableNameY":"Signal", "outputFile":"{}/napiecie.svg"},
				"2":{"variableNameX":"Simulation time", "variableNameY":"total resistance", "outputFile":"{}/opor.svg"},
				"3":{"variableNameX":"Simulation time", "variableNameY":"mx", "outputFile":"{}/magnetyzacja_mx.svg"},
				"4":{"variableNameX":"Signal", "variableNameY":"total resistance", "outputFile":"{}/opor_napiecie.svg"}
			}
	}

{
	"symulation":{
		"doSymulation":true,
		"oommfPath":"/net/archive/groups/plggspinsym/WJ/OOMMF-SWAG/oommf.tcl",
		"grant":"grant009",
		"partition":"plgrid",
		"maxSimulationTime":"4:00:00",
		"mifParameters":{
			"mxfree":						{"value":"[math.sqrt(3)/2]", "precision": 4, "type": "script", "verbous": true},
			"myfree":						{"value":0.5, "precision": 4, "type": "value", "verbous": true},
			"VProfileType":					{"value":0, "precision": 0, "type": "value", "verbous": true},
			"Voltage":						{"value":"[0.0]", "precision": 4, "type": "script", "verbous": true},
						
			"stoppingTime":					{"value":1e-8, "type": "value", "verbous": false},
			"timeStep":						{"value":1e-13, "type": "value", "verbous": false},
			"scheduleStep":					{"value":10, "type": "value", "verbous": false},
			"vecFieldScheduleStep":			{"value":1000, "type": "value", "verbous": false}
		}
	},
	"plot":{
		"doPloting":true,
		"zSlice":3,
		"framesPerSecond":5,
		"drawSingleFrames":false,
		"odtPlots":{
			"1":{"dim":"2D","variableNameX":"{Oxs_TimeDriver::Simulation time}", "variableNameY":"MF_CurrentFlowEvolver::Signal", "outputFile":"{}/napiecie.svg"},
			"2":{"dim":"2D","variableNameX":"{Oxs_TimeDriver::Simulation time}", "variableNameY":"{MF_CurrentFlowEvolver::total resistance}", "outputFile":"{}/opor.svg"},
			"3":{"dim":"2D","variableNameX":"{Oxs_TimeDriver::Simulation time}", "variableNameY":"Oxs_TimeDriver::mx", "outputFile":"{}/magnetyzacja_mx.svg"},
			"4":{"dim":"2D","variableNameX":"MF_CurrentFlowEvolver::Signal", "variableNameY":"{MF_CurrentFlowEvolver::total resistance}", "outputFile":"{}/opor_napiecie.svg"},
			"5":{"dim":"3D","variableNameX":"Oxs_TimeDriver::mx", "variableNameY":"Oxs_TimeDriver::my", "variableNameZ":"Oxs_TimeDriver::mz", "outputFile":"{}/trajektoria_m.svg"}
		},
		"headersMap":{
			"{Oxs_TimeDriver::Simulation time}":"t",
			"MF_CurrentFlowEvolver::Signal":"V",
			"{MF_CurrentFlowEvolver::total resistance}":"R",
			"Oxs_TimeDriver::mx":"$M_x$",
			"Oxs_TimeDriver::my":"$M_y$",
			"Oxs_TimeDriver::mz":"$M_z$",
			
			"Ohm":"$\\Omega$",
			"{SI units}":"V",
			"{}":"1"
		},
		"multiPlotMode":false
	}
}
